{% extends "::base.html.twig" %}

 {% block title %}
     Añadir - Complementario
 {% endblock %}

{% block ContentHeader %}
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Complementario
            <small>Añadir un Complementario</small>
        </h1>

    </section>
{% endblock %}

{% block ContentBody %}

    <!-- Main content -->
    <section class="content">

        <!-- Main row -->
        <div class="row">
            <h2></h2>
            <div class="col-md-6 col-md-offset-3">
                <div class="box box-default box-form">

                    <h1>Complementario</h1>
                    {{ form_start(edit_form) }}
                    {{ form_widget(edit_form) }}
                    <input type="submit" class="btn box-btn-submit btn-info pull-right" value="Siguiente">

                    {{ form_end(edit_form) }}

                </div>
            </div>
        </div><!-- /.row (main row) -->

    </section><!-- /.content -->


{% endblock %}
