{% extends "::base.html.twig" %}

 {% block title %}
     Añadir resultado - Complementario
 {% endblock %}

{% block ContentHeader %}
    <section class="content-header">
        <h1>
            Complementario
            <small>Añadir el resultado de un complementario</small>
        </h1>
    </section>
{% endblock %}

{% block ContentBody %}

    <section class="content">

        <div class="row">
            <h2></h2>
            <div class="col-md-6 col-md-offset-3">
                <div class="box box-default box-form">

                    <h1>Resultados de complementarios</h1>
                    {{ form_start(form) }}
                    {{ form_widget(form) }}
                    <input type="submit" class="btn box-btn-submit btn-info pull-right" value="Siguiente">
                    {{ form_end(form) }}

                </div>
            </div>
        </div>
    </section>


{% endblock %}
