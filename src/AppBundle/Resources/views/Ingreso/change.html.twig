{% extends "::base.html.twig" %}

{% block title %}
    Transferir de sala - Ingreso
{% endblock %}

{% block ContentHeader %}
    <section class="content-header">
        <h1>
            Ingreso
            <small>Transferir de sala a un paciente</small>
        </h1>
    </section>
{% endblock %}

{% block ContentBody %}
    <section class="content">
        <div class="row">
            <h2></h2>
            <div class="col-md-6 col-md-offset-3">
                <div class="box box-default box-form">

                    <h1>Transferir de sala</h1>
                    {{ form_start(form) }}
                    {{ form_widget(form) }}
                    <input type="submit" class="btn box-btn-submit btn-info pull-right" value="Siguiente">
                    {{ form_end(form) }}
                </div>
            </div>
        </div>
    </section>
{% endblock %}
