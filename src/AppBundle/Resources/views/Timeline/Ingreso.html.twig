{% if element.fechaSalida %}
    <div class="timeline-badge bg-blue">
        <i class="fa fa-hospital-o"></i>
    </div>
    <div class="timeline-panel">
        <div class="timeline-heading">
            <h4 class="timeline-title">Ingreso</h4>
            <p>
                <small class="text-muted"><i
                            class="fa fa-clock-o"></i> {{ element.fechaIngreso|date('d/m/y') }} {{ element.fechaIngreso|ago }}
                </small>
            </p>
        </div>
        <div class="timeline-body">
            <p><a href="{{ path('app_ingreso_show', {'id': element.id }) }}">Mostrar resumen de ingreso</a></p>
        </div>
    </div>
{% endif %}