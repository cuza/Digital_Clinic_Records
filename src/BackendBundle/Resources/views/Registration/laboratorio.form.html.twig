    {{ form_start(form, {'action': path('residente-registration'), 'method': 'POST'}) }}
        {{ form_widget(form) }}
        <div>
            <input type="submit" value="{{ 'registration.submit'|trans }}" />
        </div>
    {{ form_end(form) }}
{% endblock fos_user_content %}