{% extends "::base.html.twig" %}

{% block title %}AppBundle:Profile:edit{% endblock %}

{% block ContentHeader %}
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Perfil
            <small>Editar tu perfil</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="{{ path('fos_user_profile_show') }}"><i class="fa fa-medkit active"></i> Perfil</a></li>
            {#<li class="active"Crear consulta</li>#}
        </ol>
    </section>
{% endblock %}

{% block ContentBody %}
    <!-- Main content -->
    <section class="content">
        <!-- Main row -->
        <div class="row">
            <h2></h2>
            <div class="col-md-6 col-md-offset-3">
                <div class="box box-info">
                    <div class="box-header with-border">
                        <h3 class="box-title">Editar Perfil</h3>
                    </div><!-- /.box-header -->
                    <!-- form start -->
                    <div class="form-horizontal">
                        <div class="box-body">
                            {% include "FOSUserBundle:Profile:edit_content.html.twig" %}
                        </div><!-- /.box-body -->
                    </div>
                </div>
            </div>
        </div><!-- /.row (main row) -->
    </section><!-- /.content -->
{% endblock %}