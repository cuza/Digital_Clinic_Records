    {% extends ':default:backend.html.twig' %}

    {% block BackendContent %}
    <h1>Sexo</h1>

    <table class="table table-bordered">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sexo.id }}</td>
            </tr>
            <tr>
                <th>Sexo</th>
                <td>{{ sexo.sexo }}</td>
            </tr>
        </tbody>
    </table>


{{ form_start(delete_form) }}
<div class="btn-group" role="group" aria-label="...">
	
<a role="button" class="btn btn-default" data-toggle="tooltip" data-placement="top" title="Atras" href="{{ path('sexo_index') }}"><i class="fa fa-chevron-left"></i></a>
<a role="button" class="btn btn-default" data-toggle="tooltip" data-placement="top" title="Editar" href="{{ path('sexo_edit', { 'id': sexo.id }) }}"><i class="fa fa-edit"></i></a>
<button type="submit" class="btn btn-default" data-toggle="tooltip" data-placement="top" title="Eliminar" ><i class="fa fa-trash"></i></button>
</div>
{{ form_end(delete_form) }}{% endblock %}
